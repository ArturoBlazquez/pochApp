{
  "name": "pochApp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:lan": "ng serve --host=0.0.0.0",
    "build": "ng build",
    "deploy": "ng deploy --base-href=/pochApp/",
    "test": "start-server-and-test start http://localhost:4200 test:e2e",
    "test:e2e": "cypress run",
    "cy:open": "cypress open --e2e --browser=electron"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.3.0",
  "dependencies": {
    "@angular/common": "^20.3.15",
    "@angular/compiler": "^20.3.15",
    "@angular/core": "^20.3.15",
    "@angular/forms": "^20.3.15",
    "@angular/platform-browser": "^20.3.15",
    "@angular/router": "^20.3.15",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "ng-zorro-antd": "^20.4.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.3.13",
    "@angular/cli": "^20.3.13",
    "@angular/compiler-cli": "^20.3.15",
    "angular-cli-ghpages": "^2.0.3",
    "cypress": "^15.7.1",
    "jsdom": "^27.1.0",
    "start-server-and-test": "^2.1.3",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4"
  }
}
